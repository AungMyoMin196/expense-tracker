    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch expense_tracker development",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "development",
                "--verbose",
            ]
        },
        {
            "name": "Launch expense_tracker staging",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_staging.dart",
            "args": [
                "--flavor",
                "staging",
            ]
        },
        {
            "name": "Launch expense_tracker production",
            "request": "launch",
            "type": "dart",
            // "flutterMode": "release",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production",
            ]
        }
    ]
}